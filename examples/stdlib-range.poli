range:
    !!new = a, b, step :
        .current = null
        .end = b or a
        .start = b and (a or 0)
        .step = step or (.end<0 and (-1 or 1))

    !!next =:
        if not .current
            .current = .start

        elif (.step > 0 and .current <= .end) or (.step < 0 and .current >= .end)
            .current += .step

        else
            return null

        return .current

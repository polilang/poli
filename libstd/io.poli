with class from std.class
with stdio from external.C


fileWrapper =
   index! = class

   init = [self, pointer, str name, str mode]
      self.pointer = pointer
      self.name = name
      self.mode = mode

file =
   open: [str name, str mode]
      c_fprt = fopen(name, mode)
      return fileWrapper(c_fptr, name, mode)
